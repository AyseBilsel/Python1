fac = 1
for i in range(1,11):
    fac = fac * i
    print(str(i) + "!=" + str(fac))


for i in range(5):
    for j in range(10):
        print("*",end=" ")
    print()
    
print("---------\n")

for i in range(5):
    for j in range(i+1):
        print(j+1,end=" ")
    print()

print("---------\n")
